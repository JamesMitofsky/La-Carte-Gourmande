{
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.52.3"
  },
  "scripts": {
    "watch:eleventy": "eleventy --serve",
    "sass": "sass assets/css/scss/functionalStyles.scss:_site/assets/css/functionalStyles.css",
    "watch:sass": "npm run sass -- --watch",
    "start": "npm-run-all sass --parallel watch:*",
    "build": "npm run sass && eleventy"
  }
}
