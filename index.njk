---
layout: default
title: Home
---

<object
  aria-hidden="true"
  id="loading"
  type="image/svg+xml"
  data="assets/loading.svg"
></object>

<div aria-hidden="true" id="container">{% include "test.svg" %}</div>

<section class="carosel">
  {%- for card in collections.cards -%}
  <div class="card" id="{{ card.data.cssId }}-card">
    <h2 id="card-hero-txt">{{ card.data.title }}</h2>
    <p>{{ card.templateContent | safe }}</p>
  </div>
  {%- endfor -%}
</section>
